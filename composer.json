{
	"name":"cnajjar/weather-get",
	"description":"Contao extension for displaying weather using yahoo / openweathermap / forecast.io ",
	"keywords":["contao", "composer","weather","wetter","yahoo weather","openweathermap","forecast.io"],
	"type":"contao-module",
	"license":"MIT",
	"authors":[
		{
			"name":"Christian Najjar",
			"email":"info@christian-najjar.de",
			"homepage":"http://contao.christian-najjar.de",
			"role":"Developer"
		}
	],
	"require":{
		"php":">=5.3",
		"contao/core":">=3.5.1,<4.2",
		"contao-community-alliance/composer-plugin": "~2.0"
	},
	"replace": {
		"contao-legacy/weatherGET": "self.version"
	},

	
	"extra":{
		"contao": {
			"sources":{
				"src/system/modules/weather-get":"system/modules/weather-get"
			},
			"runonce":[
				"runonce/logger.php"
			]
		},
		"branch-alias": {
			"dev-master": "0.8.0-stable"
		}
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
